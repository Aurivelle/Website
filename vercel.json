{
  "version": 2,
  "builds": [
    {
      "src": "api/dailyChallenge.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/posts.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/notifications.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/daily-challenges",
      "dest": "/api/dailyChallenge.js"
    },
    {
      "src": "/api/posts",
      "dest": "/api/posts.js"
    },
    {
      "src": "/api/notifications",
      "dest": "/api/notifications.js"
    }
  ]
}
